# Check recently accessed DLL files - may show where it loaded from
Get-ChildItem "C:\" -Recurse -Filter "Microsoft.Windows.Azure.Management.Compute.dll" -ErrorAction SilentlyContinue

# Also check temp/install staging folders
Get-ChildItem "C:\Windows\Temp" -Recurse -Filter "*Compute*" -ErrorAction SilentlyContinue
Get-ChildItem "$env:TEMP" -Recurse -Filter "*Compute*" -ErrorAction SilentlyContinue
