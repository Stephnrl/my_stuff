# Check GAC on the 2019 server
Get-ChildItem "C:\Windows\Microsoft.NET\assembly" -Recurse -Filter "*Compute*" -ErrorAction SilentlyContinue

# Check whole machine
Get-ChildItem "C:\" -Recurse -Filter "Microsoft.Windows.Azure.Management.Compute.dll" -ErrorAction SilentlyContinue

# Check what .NET versions are installed
Get-ChildItem "C:\Windows\Microsoft.NET\Framework*" | Select-Object Name
