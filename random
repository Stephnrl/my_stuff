# Check if AppLocker is configured and what mode
Get-AppLockerPolicy -Effective | Select-Object -ExpandProperty RuleCollections

# Check AppLocker service
Get-Service -Name AppIDSvc | Select-Object Name, Status, StartType

# Check AppLocker event log for blocked events - this is the key one
Get-WinEvent -LogName "Microsoft-Windows-AppLocker/DLL" -ErrorAction SilentlyContinue | 
  Where-Object { $_.Message -like "*Compute*" } | 
  Select-Object TimeCreated, Message
