# Check if Defender blocked or quarantined anything
Get-MpThreatDetection | Select-Object ActionSuccess, DomainUser, ProcessName, Resources, InitialDetectionTime

# Check Defender event log
Get-WinEvent -LogName "Microsoft-Windows-Windows Defender/Operational" | 
  Where-Object { $_.Message -like "*Compute*" -or $_.Message -like "*blocked*" } |
  Select-Object TimeCreated, Message



# This often catches .NET assembly load failures with more detail
Get-WinEvent -LogName "Application" | 
  Where-Object { $_.Message -like "*Compute*" -or $_.Message -like "*assembly*" } |
  Select-Object TimeCreated, LevelDisplayName, Message



# Check if SRP is configured (older alternative to AppLocker)
Get-ItemProperty "HKLM:\SOFTWARE\Policies\Microsoft\Windows\Safer\CodeIdentifiers" -ErrorAction SilentlyContinue

Get-WinEvent -LogName "Application" | 
  Where-Object { $_.ProviderName -like "*NET Runtime*" } |
  Select-Object TimeCreated, Message -First 20
